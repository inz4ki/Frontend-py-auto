{"ast":null,"code":"import { http } from '../service/api';\nexport default {\n  name: 'ModalCadastro',\n  props: {\n    visible: Boolean,\n    variant: String\n  },\n  data() {\n    return {\n      OpenClose: this.visible,\n      tempEtapa: {\n        nome_etapa: '',\n        acao: '',\n        tempo_execucao: '',\n        digitar: '',\n        arquivo: '',\n        fk_id_tarefa: this.$parent.pk_id_tarefa\n      }\n    };\n  },\n  methods: {\n    OpenCloseFun() {\n      this.OpenClose = !this.OpenClose;\n    },\n    salvarEtapa() {\n      var form = '';\n      form = new FormData();\n      form.append('arquivo', this.tempEtapa.arquivo);\n      form.append('nome', this.tempEtapa.arquivo.name);\n      http.post(`etapa/salvar`, this.tempEtapa, {\n        headers: {\n          'Content-Type': 'multipart/form-data'\n        }\n      }).then(response => {\n        alert(\"Etapa Cadastrada\");\n        this.$parent.listarEtapas();\n        this.tempEtapa.nome_etapa = '';\n        this.tempEtapa.acao = '';\n        this.tempEtapa.tempo_execucao = '';\n        this.tempEtapa.digitar = '';\n        this.arquivo = '';\n        this.OpenCloseFun();\n      });\n    },\n    novoArquivo(event) {\n      this.tempEtapa.arquivo = event.target.files[0];\n    }\n  },\n  watch: {\n    visible: function (newVal, oldVal) {\n      this.OpenClose = newVal;\n      console.log('new ' + newVal + '==' + oldVal);\n    }\n  }\n};","map":{"version":3,"mappings":"AACA,SAASA,IAAG,QAAS,gBAAe;AACpC,eAAe;EACbC,IAAI,EAAE,eAAe;EACrBC,KAAK,EAAE;IACLC,OAAO,EAAEC,OAAO;IAChBC,OAAO,EAAEC;EACX,CAAC;EACDC,IAAI,GAAG;IACL,OAAO;MACLC,SAAS,EAAE,IAAI,CAACL,OAAO;MACvBM,SAAS,EAAE;QACTC,UAAU,EAAE,EAAE;QACdC,IAAI,EAAE,EAAE;QACRC,cAAc,EAAE,EAAE;QAClBC,OAAO,EAAE,EAAE;QACXC,OAAO,EAAE,EAAE;QACXC,YAAY,EAAE,IAAI,CAACC,OAAO,CAACC;MAC7B;IAEF;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,YAAY,GAAG;MACb,IAAI,CAACX,SAAQ,GAAI,CAAC,IAAI,CAACA,SAAS;IAClC,CAAC;IACDY,WAAW,GAAG;MACZ,IAAIC,IAAG,GAAI,EAAC;MACZA,IAAG,GAAI,IAAIC,QAAQ,EAAC;MACpBD,IAAI,CAACE,MAAM,CAAC,SAAS,EAAE,IAAI,CAACd,SAAS,CAACK,OAAO;MAC7CO,IAAI,CAACE,MAAM,CAAC,MAAM,EAAE,IAAI,CAACd,SAAS,CAACK,OAAO,CAACb,IAAI;MAC/CD,IAAI,CAACwB,IAAI,CAAE,cAAa,EAAE,IAAI,CAACf,SAAS,EAAE;QACxCgB,OAAO,EAAE;UACP,cAAc,EAAE;QAClB;MACF,CAAC,CAAC,CAACC,IAAI,CAACC,QAAO,IAAK;QAClBC,KAAK,CAAC,kBAAkB;QACxB,IAAI,CAACZ,OAAO,CAACa,YAAY,EAAE;QAC3B,IAAI,CAACpB,SAAS,CAACC,UAAS,GAAI,EAAC;QAC7B,IAAI,CAACD,SAAS,CAACE,IAAG,GAAI,EAAC;QACvB,IAAI,CAACF,SAAS,CAACG,cAAa,GAAI,EAAC;QACjC,IAAI,CAACH,SAAS,CAACI,OAAM,GAAI,EAAC;QAC1B,IAAI,CAACC,OAAO,GAAC,EAAC;QACd,IAAI,CAACK,YAAY,EAAC;MACpB,CAAC;IACH,CAAC;IACDW,WAAW,CAACC,KAAK,EAAC;MAChB,IAAI,CAACtB,SAAS,CAACK,OAAM,GAAIiB,KAAK,CAACC,MAAM,CAACC,KAAK,CAAC,CAAC;IAC/C;EACF,CAAC;EACDC,KAAK,EAAE;IACL/B,OAAO,EAAE,UAAUgC,MAAM,EAAEC,MAAM,EAAE;MACjC,IAAI,CAAC5B,SAAQ,GAAI2B,MAAM;MACvBE,OAAO,CAACC,GAAG,CAAC,MAAK,GAAIH,MAAK,GAAI,IAAG,GAAIC,MAAM,CAAC;IAC9C;EACF;AACF","names":["http","name","props","visible","Boolean","variant","String","data","OpenClose","tempEtapa","nome_etapa","acao","tempo_execucao","digitar","arquivo","fk_id_tarefa","$parent","pk_id_tarefa","methods","OpenCloseFun","salvarEtapa","form","FormData","append","post","headers","then","response","alert","listarEtapas","novoArquivo","event","target","files","watch","newVal","oldVal","console","log"],"sourceRoot":"","sources":["C:\\Users\\vinicius.martins\\Desktop\\pasta\\teste\\projeto-automacao\\src\\components\\ModalCadastro.vue"],"sourcesContent":["<script>\r\nimport { http } from '../service/api'\r\nexport default {\r\n  name: 'ModalCadastro',\r\n  props: {\r\n    visible: Boolean,\r\n    variant: String,\r\n  },\r\n  data() {\r\n    return {\r\n      OpenClose: this.visible,\r\n      tempEtapa: {\r\n        nome_etapa: '',\r\n        acao: '',\r\n        tempo_execucao: '',\r\n        digitar: '',\r\n        arquivo: '',\r\n        fk_id_tarefa: this.$parent.pk_id_tarefa\r\n      },\r\n      \r\n    }\r\n  },\r\n  methods: {\r\n    OpenCloseFun() {\r\n      this.OpenClose = !this.OpenClose;\r\n    },\r\n    salvarEtapa() {\r\n      var form = ''\r\n      form = new FormData()\r\n      form.append('arquivo', this.tempEtapa.arquivo)\r\n      form.append('nome', this.tempEtapa.arquivo.name)\r\n      http.post(`etapa/salvar`, this.tempEtapa, {\r\n        headers: {\r\n          'Content-Type': 'multipart/form-data'\r\n        }\r\n      }).then(response => {\r\n        alert(\"Etapa Cadastrada\")\r\n        this.$parent.listarEtapas();\r\n        this.tempEtapa.nome_etapa = ''\r\n        this.tempEtapa.acao = ''\r\n        this.tempEtapa.tempo_execucao = ''\r\n        this.tempEtapa.digitar = ''\r\n        this.arquivo=''\r\n        this.OpenCloseFun()\r\n      })\r\n    },\r\n    novoArquivo(event){\r\n      this.tempEtapa.arquivo = event.target.files[0]\r\n    }\r\n  },\r\n  watch: {\r\n    visible: function (newVal, oldVal) {\r\n      this.OpenClose = newVal;\r\n      console.log('new ' + newVal + '==' + oldVal);\r\n    }\r\n  }\r\n}\r\n</script>\r\n<template>\r\n  <div v-if=\"OpenClose\" class=\"modal fade show\" style=\"display:block\" tabindex=\"-1\" aria-labelledby=\"exampleModalLabel\"\r\n    aria-modal=\"true\" role=\"dialog\">\r\n    <div class=\"modal-dialog\">\r\n      <div class=\"modal-content fundo\">\r\n        <div class=\"modal-head\">\r\n          <h1>Criar Etapa</h1>\r\n        </div>\r\n        <div class=\"modal-body\">\r\n          <div class=\"col-md-12\">\r\n            <div>\r\n              <label>Nome da Etapa:</label>\r\n              <input v-model=\"tempEtapa.nome_etapa\" class=\"form-control\" name=\"nome\">\r\n            </div>\r\n            <div>\r\n              <label>Print do item que você gostaria de interagir:</label>\r\n              <input class=\"form-control\" type=\"file\" accept=\"image/png, image/gif, image/jpeg\" @change=\"novoArquivo\">\r\n              <label>Ação que gostaria de Fazer:</label>\r\n              <div>\r\n                <select v-model=\"tempEtapa.acao\" class=\"form-select\">\r\n                  <option value=\"Clicar\">Clicar</option>\r\n                  <option value=\"Clicar 2X\">Clicar 2X</option>\r\n                  <option value=\"digitar\">Clicar e Digitar</option>\r\n                </select>\r\n              </div>\r\n              <div v-if=\"tempEtapa.acao != 'digitar'\">\r\n              </div>\r\n              <div v-else>\r\n                <label>O que deve ser digitado?</label>\r\n                <div>\r\n                  <input v-model=\"tempEtapa.digitar\" type=\"text\" class=\"form-control\">\r\n                </div>\r\n              </div>\r\n              <div>\r\n                <label>Tempo para Executar:</label>\r\n                <select v-model=\"tempEtapa.tempo_execucao\" class=\"form-select\">\r\n                  <option value=\"5\">5 Segundos</option>\r\n                  <option value=\"10\">10 Segundos</option>\r\n                  <option value=\"15\">15 Segundos</option>\r\n                  <option value=\"20\">20 Segundos</option>\r\n                  <option value=\"25\">25 Segundos</option>\r\n                  <option value=\"30\">30 Segundos</option>\r\n                  <option value=\"35\">35 Segundos</option>\r\n                </select>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <div class=\"text-center botao\">\r\n            <button @click=\"salvarEtapa()\" class=\"btn btn-danger\">Salvar</button>\r\n          </div>\r\n        </div>\r\n        <div class=\"modal-footer\">\r\n          <button type=\"button\" @click=\"OpenCloseFun()\" :class=\"'btn btn-' + variant\">Fechar</button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n  <button type=\"button\" @click=\"OpenCloseFun()\" :class=\"'btn btn-' + variant\">Incluir</button>\r\n</template>\r\n<style scoped>\r\nlabel {\r\n  margin-top: 10px;\r\n}\r\n\r\n.botao {\r\n  margin-top: 10px;\r\n}\r\n\r\n.fundo {\r\n  background-color: rgb(224, 224, 224);\r\n  border: 2px solid black;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}