{"ast":null,"code":"import ModalCadastro from '../components/ModalCadastro.vue';\nexport default {\n  components: {\n    ModalCadastro\n  },\n  data() {\n    return {\n      OpenClose: this.visible\n    };\n  },\n  methods: {\n    OpenCloseFun() {\n      this.OpenClose = !this.OpenClose;\n    }\n  },\n  watch: {\n    visible: function (newVal, oldVal) {\n      this.OpenClose = newVal;\n      console.log('new ' + newVal + '==' + oldVal);\n    }\n  }\n};","map":{"version":3,"mappings":"AACA,OAAOA,aAAY,MAAO,iCAAgC;AAE1D,eAAe;EACbC,UAAU,EAAE;IACVD;EACF,CAAC;EACDE,IAAI,GAAG;IACL,OAAO;MACLC,SAAS,EAAE,IAAI,CAACC;IAClB;EACF,CAAC;EACDC,OAAO,EAAC;IACNC,YAAY,GAAE;MACZ,IAAI,CAACH,SAAQ,GAAI,CAAC,IAAI,CAACA,SAAS;IAClC;EACF,CAAC;EACDI,KAAK,EAAC;IACJH,OAAO,EAAE,UAASI,MAAM,EAACC,MAAM,EAAC;MAC9B,IAAI,CAACN,SAAQ,GAAIK,MAAM;MACvBE,OAAO,CAACC,GAAG,CAAC,MAAK,GAAIH,MAAK,GAAI,IAAG,GAAIC,MAAM,CAAC;IAC9C;EACF;AACF","names":["ModalCadastro","components","data","OpenClose","visible","methods","OpenCloseFun","watch","newVal","oldVal","console","log"],"sourceRoot":"","sources":["C:\\Users\\vinicius.martins\\Desktop\\pasta\\teste\\projeto-automacao\\src\\views\\HomeView.vue"],"sourcesContent":["<script>\nimport ModalCadastro from '../components/ModalCadastro.vue'\n\nexport default {\n  components: {\n    ModalCadastro,\n  },\n  data() {\n    return {\n      OpenClose: this.visible\n    }\n  },\n  methods:{\n    OpenCloseFun(){\n      this.OpenClose = !this.OpenClose;\n    }\n  },\n  watch:{\n    visible: function(newVal,oldVal){\n      this.OpenClose = newVal;\n      console.log('new ' + newVal + '==' + oldVal);\n    }\n  }\n}\n</script>\n\n<template>\n  <h1>Criação de Tarefa</h1>\n  <P>Primeiro passo</P>\n  <hr>\n  <ModalCadastro class=\"row\" variant=\"primary\">\n    <!-- <form> -->\n      <div class=\"col-md-12\">\n        <div>\n          <input class=\"form-control\" name=\"nome\" placeholder=\"Nome do Passo\">\n        </div>\n        <div>\n          <label>Print do item que você gostaria de interagir</label>\n          <input class=\"form-control\" type=\"file\" @change=\"novoArquivo\">\n          <label>Ação que gostaria de Fazer</label>\n          <select class=\"form-control\">\n            <option value=\"mover\">Mover até o arquivo</option>\n            <option value=\"click\">Mover e Clicar</option>\n            <option value=\"click\">Mover e Clicar 2X</option>\n          </select>\n        </div>\n      </div> \n      <button @click=\"OpenCloseFun()\" class=\"btn btn-danger\">Salvar</button>\n    <!-- </form> -->\n  </ModalCadastro>\n  <div>\n    <hr>\n    <p>Segundo passo</p>\n    <ModalCadastro class=\"row\" variant=\"primary\">\n\n    </ModalCadastro>\n  </div>\n</template>\n\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}