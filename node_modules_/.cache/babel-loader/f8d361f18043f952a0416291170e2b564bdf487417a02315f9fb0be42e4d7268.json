{"ast":null,"code":"import { http } from '../service/api';\nexport default {\n  name: 'ModalCadastro',\n  props: {\n    visible: Boolean,\n    variant: String\n  },\n  data() {\n    return {\n      OpenClose: this.visible,\n      tempEtapa: {\n        nome_etapa: '',\n        acao: '',\n        tempo_execucao: '',\n        digitar: '',\n        fk_id_tarefa: this.$parent.pk_id_tarefa\n      },\n      arquivo: ''\n    };\n  },\n  methods: {\n    OpenCloseFun() {\n      this.OpenClose = !this.OpenClose;\n    },\n    salvarEtapa() {\n      var form = '';\n      form = new FormData();\n      form.append('imagem', this.arquivo);\n      form.append('nome', this.arquivo.name);\n      // form.append('nome_etapa', this.tempEtapa.nome_etapa)\n      // form.append('acao', this.tempEtapa.acao)\n      // form.append('tempo_execucao', this.tempEtapa.tempo_execucao)\n      // form.append('digitar', this.tempEtapa.digitar)\n      // form.append('fk_id_tarefa', this.tempEtapa.fk_id_tarefa)\n\n      http.post(`etapa/salvar`, this.form, {\n        headers: {\n          'Content-Type': 'multipart/form-data'\n        }\n      }).then(response => {\n        alert(\"Etapa Cadastrada\");\n        console.log(response);\n        // this.$parent.listarEtapas();\n        // this.tempEtapa.nome_etapa = ''\n        // this.tempEtapa.acao = ''\n        // this.tempEtapa.tempo_execucao = ''\n        // this.tempEtapa.digitar = ''\n        // this.tempEtapa.arquivo = ''\n        this.OpenCloseFun();\n      });\n    },\n    novoArquivo(event) {\n      console.log(event);\n      this.arquivo = event.target.files[0];\n      console.log(this.arquivo);\n    }\n  },\n  watch: {\n    visible: function (newVal, oldVal) {\n      this.OpenClose = newVal;\n      console.log('new ' + newVal + '==' + oldVal);\n    }\n  }\n};","map":{"version":3,"mappings":"AACA,SAASA,IAAG,QAAS,gBAAe;AACpC,eAAe;EACbC,IAAI,EAAE,eAAe;EACrBC,KAAK,EAAE;IACLC,OAAO,EAAEC,OAAO;IAChBC,OAAO,EAAEC;EACX,CAAC;EACDC,IAAI,GAAG;IACL,OAAO;MACL