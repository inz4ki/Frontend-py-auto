{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport ModalCadastro from '../components/ModalCadastro.vue';\nimport { http } from '../service/api';\nimport draggable from 'vuedraggable';\nexport default {\n  components: {\n    draggable,\n    ModalCadastro\n  },\n  data() {\n    return {\n      pk_id_tarefa: this.$route.params.pk_id_tarefa,\n      etapas: [],\n      dragging: false,\n      tarefa: '',\n      key: ''\n    };\n  },\n  props: ['pk_id_tarefa'],\n  mounted() {\n    this.listarEtapas();\n    http.get(`tarefa/${this.pk_id_tarefa}`).then(response => {\n      this.tarefa = response.data;\n    });\n  },\n  methods: {\n    listarEtapas() {\n      http.get(`etapas/${this.pk_id_tarefa}`).then(response => {\n        this.etapas = response.data;\n      });\n    },\n    deletarTarefa(tarefa) {\n      http.delete(`tarefa/${tarefa.pk_id_tarefa}`, {\n        data: tarefa\n      }).then(response => {\n        alert(\"Tarefa excluida com sucesso\");\n        this.$router.push('/');\n        return response;\n      }).catch(error => {\n        if (error.response.status = 404) {\n          alert('Erro ao Excluir');\n        }\n      });\n    },\n    deletarEtapa(etapa) {\n      http.delete(`etapa/${etapa.pk_id_etapa}`, {\n        data: etapa\n      }).then(response => {\n        alert(\"Etapa excluida com sucesso\");\n        this.listarEtapas();\n        return response;\n      }).catch(error => {\n        if (error.response.status = 404) {\n          alert('Erro ao Excluir');\n        }\n      });\n    }\n  }\n};","map":{"version":3,"mappings":";AACA,OAAOA,aAAY,MAAO,iCAAgC;AAC1D,SAASC,IAAG,QAAS,gBAAe;AACpC,OAAOC,SAAQ,MAAO,cAAa;AAEnC,eAAe;EACbC,UAAU,EAAE;IACVD,SAAS;IACTF;EAEF,CAAC;EACDI,IAAI,GAAG;IACL,OAAO;MACLC,YAAY,EAAE,IAAI,CAACC,MAAM,CAACC,MAAM,CAACF,YAAY;MAC7CG,MAAM,EAAE,EAAE;MACVC,QAAQ,EAAC,KAAK;MACdC,MAAM,EAAE,EAAE;MACVC,GAAG,EAAC;IACN;EACF,CAAC;EACDC,KAAK,EAAE,CAAC,cAAc,CAAC;EACvBC,OAAO,GAAG;IACR,IAAI,CAACC,YAAY,EAAE;IACnBb,IAAI,CAACc,GAAG,CAAE